<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_inventory_number" model="ir.sequence">
            <field name="name">Inventory Numbers</field>
            <field name="code">inventory.number</field>
            <field name="prefix">INV</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_plan_number" model="ir.sequence">
            <field name="name">Plan Numbers</field>
            <field name="code">plan.number</field>
            <field name="prefix">PLN/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>